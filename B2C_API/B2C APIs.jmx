		apiOut.write(System.getProperty(&quot;line.separator&quot;));
		apiOut.close();
		apiWrite.close();
	}
}
else {
	print(&quot;\n-VERIFICATION OF API SAMPLE :: &quot;+apiSample+&quot; AT &quot;+apiRunDateTime+&quot;\n&quot;);
    	print(apiReqData+&quot;\n\n&quot;);
