    	apiOut.write(apiRunDateTime+&quot;;&quot;+apiThreadGroup+&quot;;&quot;+apiSample+&quot;;&quot;+apiResultCode);
	apiOut.write(&quot;;&quot;+apiResultDesc+&quot;;&quot;+apiHost+apiPath+&quot;;&quot;+apiResData404);
