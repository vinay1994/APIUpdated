		apiWrite.close();
	}
	else {
		print(&quot;\n--++--++-- [MSG@ND] VERIFICATION OF API SAMPLE :: &quot;+apiSample+&quot; AT &quot;+apiRunDateTime+&quot; :: ----+++----+++----\n&quot;);
    		print(apiReqData+&quot;\n--++--++-- [MSG@ND] GOT RESPONSE ----+++----+++----&quot;);
